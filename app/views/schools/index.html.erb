<h5>Schools</h5>
 <table class="table table-striped">
    <thead>
      <tr>
        <th width="225">Name</th>
        <th width="150">City</th>
        <th width="150">State</th>
      </tr>
    </thead>
    <tbody>
      <% @schools.each do |school| %>
        <tr>
          <td><%= link_to school.name, school_path(school) %></td>
          <td><%= school.city %></td>
          <td><%= school.state %></td>
        </tr>
      <% end %>
    </tbody>
  </table>